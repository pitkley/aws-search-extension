{
  "enabledManagers": [
    "git-submodules",
  ],
  "git-submodules": {
    "enabled": true,
  },
  "packageRules": [
    {
      "managers": ["git-submodules"],
      "matchPackagePatterns": [
        "^index-sources/",
      ],
      "automerge": true,
      "automergeType": "branch",
    },
  ],
}
